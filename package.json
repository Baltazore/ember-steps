    "prettier": "^1.5.3",
    "testdouble-chai": "^0.5.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "debug": "^2.6.8",
    "ember-cli-babel": "^6.7.2",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://alexlafroscia.com/ember-steps/"
